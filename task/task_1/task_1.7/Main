import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();
        int c = scan.nextInt();
        long volume = (a*b*c*1000);
        System.out.println("В бассейне "+ volume+" литров");
        }
}
