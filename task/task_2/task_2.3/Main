public class Main {
    public static void main(String[] args) {
        int c=8;
        for (int a=0; a<10; a++) {
            for (int b=0; b<10; b++) {
                if (b<=a) {
                    System.out.print(c+" ");
                }
            }
            System.out.println();
        }
    }
}
