public class Main {
    public static void main(String[] args) {
        int[] array = {2, 3, 3, 1, 1, 4};
        int s = 0;
        int i =0;
        while (s <= 10){
            s += array[i]; i++;
        }
        {
            System.out.println("Сложить " + (i) + " элемент(ов/а)");
        }
    }
}
