import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        double b,c;
        int d;
        b = 9.0;
        c = 5.0;
        d = 32;
        double f = b/c*a+d;
        System.out.println("По Фаренгейту "+ f);
        }
}
