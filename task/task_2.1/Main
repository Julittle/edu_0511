public class Main {
    public static void main(String[] args) {
        String[] mama = {"Мама", "Мыла", "Раму"};
        for (int a=0;a<3;a++){
            for (int b=0;b<3;b++){
                for (int c=0;c<3;c++){
                    if (a==b) continue;
                        if (c==a) continue;
                            if (c==b) continue;
                    System.out.println(" " +mama[a]+mama[b]+mama[c]);
                }
            }
        }
    }
}
