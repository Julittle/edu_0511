public class Main {
    public static void main(String[] args) {
       int i = 1;
       int j = 1;
        while (i!=11){
            for (int k = 1; k<=10; k++){
                j=i*k;
                System.out.println(j+" ");
            }
            System.out.println(" ");
            i++;
        }
    }
}

